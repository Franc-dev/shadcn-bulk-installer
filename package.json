{
  "name": "shadcn-bulk-installer",
  "version": "1.0.1",
  "description": "Bulk installer for shadcn/ui components with interactive CLI",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Franc-dev/shadcn-bulk-installer.git"
  },
  "keywords": [
    "shadcn",
    "ui",
    "components",
    "installer",
    "cli",
    "bulk",
    "nextjs"
  ],
  "author": "Francis Mwaniki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Franc-dev/shadcn-bulk-installer/issues"
  },
  "homepage": "https://github.com/Franc-dev/shadcn-bulk-installer#readme",

  "main": "dist/index.js",
  "bin": {
    "shadcn-bulk": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "link:dev": "npm link",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "execa": "^5.1.1",
    "inquirer": "^8.2.6",
    "listr": "^0.14.3",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "@types/listr": "^0.14.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}